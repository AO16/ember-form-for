{{#form-field propertyName
    object=object
    form=form
    control="form-controls/select"
    update=(action update) as |f|}}
  {{f.label required=required}}
  {{f.control
      accesskey=accesskey
      autofocus=autofocus
      dir=dir
      disabled=disabled
      groupLabelPath=groupLabelPath
      hidden=hidden
      lang=lang
      multiple=multiple
      options=options
      optionValuePath=optionValuePath
      optionLabelPath=optionLabelPath
      required=required
  }}
  {{f.errors}}
  {{f.hint}}
{{/form-field}}
